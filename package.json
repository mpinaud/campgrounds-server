    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "nodemon": "^1.18.7"
  },
  "dependencies": {
    "dotenv": "^6.2.0",
    "graphql-yoga": "^1.16.7",
    "morgan": "^1.9.1",
    "prisma-binding": "^2.2.8",
    "yarn-run-all": "^3.1.1"
  },
  "babel": {
    "presets": [
      "env"
    ]
  }
}
